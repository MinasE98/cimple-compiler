program test1
	declare x, y, z;
	procedure toString(in var3, inout var4, in var5)
	declare var3,var4,var5;
	{
		var3:=var3+var4;
		switchcase
		case(var3<>2)
			print(var3);
		case(var3>1)
			print(var4);
		case(var3<6)
			print(var5);
		default{
			var5:=var3+var4;
			print(var5);
		};
		
	}
	#Main#
	{
	 x:=3;
	 y:=2;
	 z:=1;
	 call toString(in x,inout y,in z);
	}.
